{
  "name": "react-starter",
  "version": "1.0.0",
  "description": "React project starter with Prettier, ESlint, Commitlint, Storybook and Jest.",
  "repository": "https://github.com/douglasdemoura/react-starter",
  "author": "Douglas Moura <douglas.ademoura@gmail.com> (https://douglasmoura.dev)",
  "license": "MIT",
  "source": "src/index.html",
  "scripts": {
    "dev": "vite",
    "build": "tsc && vite build",
    "preview": "vite preview",
    "storybook": "start-storybook -p 6006",
    "build-storybook": "build-storybook",
    "generate": "yarn plop --plopfile generators/plopfile.js",
    "test": "jest",
    "lint": "eslint src",
    "typecheck": "tsc --noEmit",
    "prepare": "husky install"
  },
  "dependencies": {
    "@react-hook/merged-ref": "^1.3.2",
    "@stitches/react": "^1.2.7",
    "@vitejs/plugin-react": "^1.2.0",
    "axios": "^0.26.1",
    "lodash": "^4.17.21",
    "react": "^17.0.2",
    "react-dom": "^17.0.2",
    "vite": "^2.8.4"
  },
  "devDependencies": {
    "@commitlint/cli": "^16.2.1",
    "@commitlint/config-conventional": "^16.2.1",
    "@storybook/addon-actions": "^6.4.19",
    "@storybook/addon-essentials": "^6.4.19",
    "@storybook/addon-links": "^6.4.19",
    "@storybook/react": "^6.4.19",
    "@swc/core": "^1.2.144",
    "@swc/jest": "^0.2.17",
    "@testing-library/jest-dom": "^5.16.2",
    "@testing-library/react": "^12.1.3",
    "@testing-library/user-event": "^13.5.0",
    "@types/jest": "^27.4.1",
    "@types/lodash": "^4.14.180",
    "@types/react": "^17.0.39",
    "@types/react-dom": "^17.0.11",
    "@typescript-eslint/eslint-plugin": "^5.12.1",
    "@typescript-eslint/parser": "^5.12.1",
    "@typescript-eslint/utils": "^5.12.1",
    "eslint": "^8.9.0",
    "eslint-config-prettier": "^8.4.0",
    "eslint-plugin-import-helpers": "^1.2.1",
    "eslint-plugin-jest": "^26.1.1",
    "eslint-plugin-jest-dom": "^4.0.1",
    "eslint-plugin-jsx-a11y": "^6.5.1",
    "eslint-plugin-prettier": "^4.0.0",
    "eslint-plugin-react": "^7.28.0",
    "eslint-plugin-react-hooks": "^4.3.0",
    "eslint-plugin-storybook": "^0.5.7",
    "husky": ">=7",
    "identity-obj-proxy": "^3.0.0",
    "jest": "^27.5.1",
    "jest-watch-typeahead": "^1.0.0",
    "lint-staged": ">=12",
    "msw": "^0.39.2",
    "plop": "^3.0.5",
    "prettier": "^2.5.1",
    "typescript": "^4.5.5"
  },
  "lint-staged": {
    "*.{ts,tsx,jsx,js}": [
      "eslint --fix",
      "jest --findRelatedTests --bail"
    ]
  },
  "msw": {
    "workerDirectory": "public"
  }
}
